#run zshrc
alias sz='source ~/.zshrc'

#ファイル操作
alias ll='ls -al'

#処理確認
alias rm='rm -i'
alias mv='mv -i'
alias cp='cp -i'

# tmux
alias tm='tmux'

#vim
alias v='vim'
alias vz='vim ~/.zshrc'
alias va='vim ~/.zsh_alias'
alias vv='vim ~/.vimrc'

#jupyter
alias jn='jupyter notebook --NotebookApp.use_redirect_file=False'

#cdしたあとにls
cdls ()
{
    \cd "$@" && ll
}
alias d="cdls"

# python
alias python='python3'
alias pip='pip3'

# venv
alias sv='source .venv/bin/activate'

# githubを開く
alias github="xdg-open https://github.com/fujitako03"

